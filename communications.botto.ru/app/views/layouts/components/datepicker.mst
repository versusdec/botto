<div class="datepicker {{classlist}}" id="{{id}}">
    <label>{{label}}</label>
    <span class="mdi mdi-calendar">
        <input type="text" value="{{date}}" name="{{name}}" autocomplete="off">
        <div class="hide">
            {{#calendar date=date onchange=onselect}}{{/calendar}}
        </div>
    </span>
</div>