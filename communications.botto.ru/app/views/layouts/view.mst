<h1>
  <a class="back mdi mdi-arrow-left-circle-outline"
     href="{{back}}"></a>
  {{title}}
</h1>
{{#if tabs}}
  <ul class="tabs">
    {{#each tabs}}
      <li {{#if active}}class="active"{{/if}} {{#if disabled}}class="disabled"{{/if}}>
        <a href="{{link}}">{{title}}</a>
      </li>
    {{/each}}
  </ul>
{{/if}}
<hr>
{{{content}}}
