    <input type="text" value="{{name}}" name="name">
  </td>
  <td title="{{#i18n}}projects.fields.var{{/i18n}}">
    <input type="text" value="{{value}}" name="value">
  </td>
  <td title="{{#i18n}}table.actions{{/i18n}}">
